    { "name": "mario", "group": "style", "selectedTokenSets": { "core": "enabled", "theme/mario": "enabled" } },
    { "name": "nintendo_switch", "group": "style", "selectedTokenSets": { "core": "enabled", "theme/nintendo_switch": "enabled" } },
    { "name": "minecraft", "group": "style", "selectedTokenSets": { "core": "enabled", "theme/minecraft": "enabled" } },
