  "core": {
    "dimension": {
      "space": {
        "0": { "$type": "dimension", "$value": "0px" },
        "1": { "$type": "dimension", "$value": "4px" },
        "2": { "$type": "dimension", "$value": "8px" },
        "3": { "$type": "dimension", "$value": "12px" },
        "4": { "$type": "dimension", "$value": "16px" },
        "5": { "$type": "dimension", "$value": "20px" },
        "6": { "$type": "dimension", "$value": "24px" },
        "8": { "$type": "dimension", "$value": "32px" }
      },
      "radius": {
        "none": { "$type": "dimension", "$value": "0px" },
        "sm": { "$type": "dimension", "$value": "6px" },
        "md": { "$type": "dimension", "$value": "10px" },
        "lg": { "$type": "dimension", "$value": "16px" },
        "pill": { "$type": "dimension", "$value": "999px" }
      },
      "borderWidth": {
        "hairline": { "$type": "dimension", "$value": "1px" },
        "thick": { "$type": "dimension", "$value": "2px" }
      }
    },
    "boxShadow": {
      "elevation": {
        "1": { "$type": "boxShadow", "$value": { "type": "dropShadow", "x": "0px", "y": "2px", "blur": "8px", "spread": "0px", "color": "rgba(0,0,0,0.16)" } }
      }
    },
    "fontFamily": {
      "base": { "$type": "fontFamily", "$value": "system-ui, -apple-system, Segoe UI, Roboto, Noto Sans JP, sans-serif" },
      "mono": { "$type": "fontFamily", "$value": "ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, 'Liberation Mono', monospace" }
    },
    "fontWeight": {
      "regular": { "$type": "fontWeight", "$value": "400" },
      "medium": { "$type": "fontWeight", "$value": "500" },
      "bold": { "$type": "fontWeight", "$value": "700" }
    },
    "fontSize": {
      "md": { "$type": "fontSize", "$value": "14px" },
      "lg": { "$type": "fontSize", "$value": "16px" },
      "xl": { "$type": "fontSize", "$value": "20px" }
    },
    "typography": {
      "body": { "$type": "typography", "$value": { "fontFamily": "{fontFamily.base}", "fontWeight": "{fontWeight.regular}", "fontSize": "{fontSize.lg}", "lineHeight": "140%", "letterSpacing": "0%" } },
      "label": { "$type": "typography", "$value": { "fontFamily": "{fontFamily.base}", "fontWeight": "{fontWeight.medium}", "fontSize": "{fontSize.md}", "lineHeight": "120%", "letterSpacing": "0%" } }
